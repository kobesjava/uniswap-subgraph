#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/saihaj/Desktop/the-guild/graph-tooling/node_modules/.pnpm/solidity-coverage@0.8.2_hardhat@2.13.1/node_modules/solidity-coverage/plugins/node_modules:/Users/saihaj/Desktop/the-guild/graph-tooling/node_modules/.pnpm/solidity-coverage@0.8.2_hardhat@2.13.1/node_modules/solidity-coverage/node_modules:/Users/saihaj/Desktop/the-guild/graph-tooling/node_modules/.pnpm/solidity-coverage@0.8.2_hardhat@2.13.1/node_modules:/Users/saihaj/Desktop/the-guild/graph-tooling/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/saihaj/Desktop/the-guild/graph-tooling/node_modules/.pnpm/solidity-coverage@0.8.2_hardhat@2.13.1/node_modules/solidity-coverage/plugins/node_modules:/Users/saihaj/Desktop/the-guild/graph-tooling/node_modules/.pnpm/solidity-coverage@0.8.2_hardhat@2.13.1/node_modules/solidity-coverage/node_modules:/Users/saihaj/Desktop/the-guild/graph-tooling/node_modules/.pnpm/solidity-coverage@0.8.2_hardhat@2.13.1/node_modules:/Users/saihaj/Desktop/the-guild/graph-tooling/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/solidity-coverage@0.8.2_hardhat@2.13.1/node_modules/solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/solidity-coverage@0.8.2_hardhat@2.13.1/node_modules/solidity-coverage/plugins/bin.js" "$@"
fi
